<?xml version="1.0"?>

<launch>
<param name="use_sim_time" value="true"/>
<arg name="drone_name" default="iris"/>
  <group ns="$(arg drone_name)">
    <node name="hitl_interface" pkg="training_q_learning" type="hitl_interface.py" output="screen" >
        <param name = "hitl_interface_hz" value = "100" />
        <param name = "drone_name" value = "$(arg drone_name)" />
    </node> 
  </group>  
</launch>
